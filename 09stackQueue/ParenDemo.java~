public class ParenDemo {

    public static boolean isMatching(String s) {
	String opening = "";
	String closing = "";
	for (int i = 0; i < s.length(); i ++) {
	    if (s.substring(i, i + 1) == "[" || s.substring(i, i + 1) == "(" || s.substring(i, i + 1) == "{") {
		opening += s.substring(i, i + 1);
	    }
	    else if (s.substring(i, i + 1) == "]" || s.substring(i, i + 1) == ")" || s.substring(i, i + 1) == "}") {
		ending += s.subsring(i, i + 1);
	    }
	}
	if (opening.length() == closing.length()) {
	    while (opening.length > 0) {
		if (opening.charAt(opening.length() - 1) == closing.charAt(0)) {
		    opening = opening.substring(0, opening.length() - 1);
		    closing = closing.substring(1, closing.length());
		}
		else {
		    return false;
		}
	    }
	    return false;
	}
    }
   
